	},
	"devDependencies": {
		"ava": "^0.25.0",
		"chalk": "^2.3.2",
		"coveralls": "^3.0.0",
		"nyc": "^11.6.0",
		"open": "0.0.5",
		"pixelmatch": "^4.0.2",
		"xo": "^0.20.3"
	},
	"nyc": {
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"esnext": true,
		"semicolon": false,
		"globals": [
			"__dirname"
		],
		"rules": {
			"no-console": 1,
			"space-before-function-paren": 0
		}
	},
	"ava": {
		"failFast": true
	}
